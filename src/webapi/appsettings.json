{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "OpenIddict.*": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate":  "[{Timestamp:HH:mm:ss}] {Level:u3} {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "EnableSwagger": true,
  "Authentication": {
    "IssuerUrl": "",
    "Audience": "epp-eta",
    "Authority": "",
    "SigningKey": "",
    "NewUsersAreAdministrators": false,
    "RequireHttps": true
  },
  "CorsAllowedOrigins": "",
  "BasePath": "",
  "TrustAllSSLCerts": false
}
